{
    "name": "iamczech/easyadmin-fields-bundle",
    "version": "1.0.3",
    "description": "Extends basic EasyAdmin fields and works well with AssetMapper and Smyfony 7.4^",
    "type": "symfony-bundle",
    "keywords": [
        "symfony-ux"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Iamczech\\EasyAdminFieldsBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "xXIamCzechXx",
            "email": "75167109+xXIamCzechXx@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "symfony/framework-bundle": "^7.4",
        "easycorp/easyadmin-bundle": "^4.0"
    },
    "suggest": {
        "npm-asset/sortablejs": "JavaScript library for drag-and-drop functionality. Install via `npm install sortablejs`."
    },
    "extra": {
        "symfony": {
            "asset-mapper": {
                "paths": {
                    "assets/vendor/sortablejs": "@sortablejs"
                }
            }
        }
    }
}
